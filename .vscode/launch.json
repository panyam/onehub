    "version": "0.2.0",
    "configurations": [
      {
          "name": "Launch file",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          "program": "cmd/server.go",
          "cwd": "${workspaceFolder}",
          "dlvFlags": ["--check-go-version=false"]
      }
        ,
       {
            "name": "Remote Docker for OneHub",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 9091,
            "showLog": true,
            "host": "127.0.0.1"
      }
    ]
}
